{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "github>kakushindev/renovate-config"
    ],
    "packageRules": [
        {
            "matchDatasources": [
                "docker"
            ],
            "matchPackageNames": [
                "docker.io/library/nextcloud",
                "nextcloud"
            ],
            "versioning": "semver"
        }
    ],
    "major": {
        "enabled": false
    },
    "minor": {
        "enabled": true
    },
    "patch": {
        "enabled": true
    }
}
